{{> navbar}}
<div class="listing-detail">
  {{#each listingImages}}
    <img src="/images/house{{../listing.id}}/{{this}}" alt="House" class="listing-image"/>
  {{/each}}
  <img src="/images/calleSusannaHouse.jpg" alt="House" class="listing-image"/>
  <h1>{{listing.type}}</h1>
  <h3>Price: ${{listing.price}}</h3>
  <p>Address: {{listing.address}}</p>
  <p>Description: {{listing.description}}</p>
  <a href="/soldHomes">Back to previous sales</a>
</div>